{
  "name": "raydium-lp-cli",
  "version": "0.1.0",
  "private": true,
  "description": "Automated CLI tool for managing Raydium Liquidity Positions",
  "main": "dist/index.js",
  "bin": {
    "raydium-lp": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "rm -rf dist && tsc",
    "start": "node dist/cli.js monitor --interval 5",
    "start:prod": "node dist/cli.js monitor --interval 5",
    "lint": "eslint src --ext ts --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "monitor": "tsx src/cli.ts monitor",
    "test": "jest"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@raydium-io/raydium-sdk-v2": "^0.2.31-alpha",
    "@solana/web3.js": "1.95.8",
    "axios": "^1.7.9",
    "commander": "^12.1.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.4.7",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.67.0",
    "winston": "^3.17.0",
    "yaml": "^2.6.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/node-cron": "^3.0.11",
    "@types/node-telegram-bot-api": "^0.64.13",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
