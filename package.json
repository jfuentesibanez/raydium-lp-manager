{
  "name": "raydium-lp-manager",
  "version": "0.1.0",
  "private": true,
  "description": "Automated liquidity management system for Raydium CLMM pools on Solana",
  "workspaces": [
    "backend"
  ],
  "scripts": {
    "dev": "cd backend && npm run dev",
    "build": "cd backend && npm run build",
    "start": "cd backend && npm start",
    "monitor": "cd backend && npm run monitor",
    "install:all": "npm install && cd backend && npm install",
    "lint": "cd backend && npm run lint",
    "type-check": "cd backend && npm run type-check"
  },
  "keywords": [
    "solana",
    "raydium",
    "liquidity-pool",
    "clmm",
    "defi",
    "automation",
    "yield-farming"
  ],
  "author": "",
